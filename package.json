{
  "name": "spotify-rpc",
  "version": "1.0.0",
  "description": "spotify",
  "main": "src/main.js",
  "scripts": {
    "app": "electron src/main.js",
    "build:mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --prune=true --out=builds",
    "build:win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=builds",
    "build:linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --prune=true --out=builds",
    "build": "electron-packager . --overwrite --asar=true --all --prune=true --out=builds"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord-rpc": "github:devsnek/discord-rpc",
    "spotify.js": "github:devsnek/spotify.js"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0"
  }
}
